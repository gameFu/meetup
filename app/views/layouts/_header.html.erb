<header class="navbar navbar-fixed-top navbar-inverse">
           <div class="container">
             <%= link_to "meetup", root_path, id: "logo" %>
             <nav>
                 <ul class="nav navbar-nav pull-right">
                    <% if logged_in? %>
                    <li><span class='user_name'>Hi,<%= current_user.name %></span><%= link_to 'Logout', logout_path, method: 'delete', id: 'logout' %></li>
                    <% else %>
                    <li><%= link_to 'Login', login_path %></li>
                    <% end %>
         </ul>
       </nav>
           </div>
       </header>
